{
    "env": {
        "jest": true,
        "node": true,
        "es6": true
    },
    "extends": [
        "plugin:vue/essential",
        "@vue/prettier"
    ],
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "rules": {
        "semi": ["error", "always"],
        "prettier/prettier": ["error", { "singleQuote": true }]
    }
}